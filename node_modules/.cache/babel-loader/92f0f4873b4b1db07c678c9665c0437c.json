tal=true;logArray();\"> Calculate Total </button>\n  <p v-show=\"displayTotal\">\n    Subtotal: ${{subtotal}}<br>\n    Grandtotal: ${{grandTotal}}\n  </p>\n</div>\n</template>\n\n<script>\nexport default {\n  name: \"Basket\",\n  data() {\n    return {\n      displayTotal: false,\n    }\n  },\n\n  props: {\n\n    itemsSelected: {\n      type: Array\n    },\n  },\n\n  computed: {\n    subtotal: function() {\n      let tot = 0;\n      for (let order of this.itemsSelected) {\n        tot += order[1] * order[2];\n      }\n      // console.log(\"Calculate subtotal: \");\n      // console.log(tot);\n      return Math.round(tot);\n    },\n    grandTotal: function() {\n      if (this.subtotal) {\n        let tot = this.subtotal * 1.07;\n        return tot.toFixed(2);\n      }\n      return 0;\n    },\n  },\n\n  watch: {\n\n  },\n\n  methods: {\n    logArray: function() {\n      console.log(\"Items in the Basket: \");\n      console.log(this.itemsSelected);\n    }\n  },\n}\n</script>\n\n<style scoped>\nbutton {\n  background-color: darksalmon;\n  border-color: antiquewhite;\n  font-size: 30px;\n}\n\ndiv {\n  font-size: 30px;\n  font-width: bold;\n}\n</style>"],"sourceRoot":"src/components"}]}